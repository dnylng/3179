{
    "$schema": "https://vega.github.io/schema/vega/v5.json",
    "title": "Happy Countries",
    "width": 800,
    "height": 500,
    "data": [
      {
        "name": "countries",
        "url": "https://raw.githubusercontent.com/dnylng/3179/main/w9_redo/js/ne_50m_admin_0_countries.json",
        "format": {
          "type": "topojson",
          "feature": "ne_50m_admin_0_countries"
        }
      }
    ],
    "projections": [
      {
        "name": "projection",
        "type": "mercator"
      }
    ],
    "marks": [
      {
        "type": "shape",
        "from": {"data": "countries"},
        "encode": {
          "enter": {
            "fill": {"value": "#ccc"}
          }
        },
        "transform": [
          {
            "type": "geoshape",
            "projection": "projection"
          }
        ]
      }
    ]
  }
  
// https://raw.githubusercontent.com/dnylng/3179/main/w9_redo/data/world-happiness-report-2021.csv